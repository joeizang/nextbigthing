{
  "name": "template-project-nextjs-typeorm-typescript",
  "version": "0.1.0",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "migration:generate": "ts-node -P tsconfig.server.json ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node -P tsconfig.server.json ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node -P tsconfig.server.json ./node_modules/typeorm/cli.js migration:revert",
    "start": "cross-env NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "ip": "^1.1.5",
    "next": "9.3.2",
    "pg": "^7.12.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.19"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/ip": "^1.1.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.0.12",
    "@types/pg": "^7.11.2",
    "@types/react": "^16.8.17",
    "@types/react-dom": "16.8.4",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "cross-env": "^5.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
